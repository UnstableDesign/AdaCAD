{
  "name": "@angular/language-service",
  "version": "21.0.2",
  "description": "Angular - language services",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "author": "angular",
  "license": "MIT",
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >=24.0.0"
  },
  "exports": {
    ".": {
